<h1> Heyy, <%= current_user.name %> </h1>
<h2> Household Name:  <%= current_user.household.name %> </h2>

<h3> Roommates: </h3>
<% @roommates.each do |roommate| %>
  <h4> Name: </h4>
  <%= roommate.name %>
  <h4> Chores: </h4>
  <% roommate.chores.each do |chore| %>
  <%= chore.name %> -- <%= chore.date_completed %> <a href="chores/<%=chore.id%>"> edit chore </a>
  <br></br>
  <% end %>
<% end %>

<br></br>
<a href="/chores/new">Log a new chore (high five!)</a>
<br></br>
<a href="/logout">Logout</a>